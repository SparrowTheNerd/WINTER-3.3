{
    "VsCodeTaskButtons.showCounter": true,
    "VsCodeTaskButtons.tasks": [
        {
            "label": "$(notebook-state-success)",
            "task": "Build STM",
            "tooltip": "Build"
        },
        {
            "label": "$(arrow-right)",
            "tasks": [
                {
                    "label": "Upload via ST-Link",
                    "task": "Flash STM",
                    "description": "Uploads to STM32 Using ST-Link Debugger"
                },
                {
                    "label": "Upload via DFU",
                    "task": "Flash DFU",
                    "description": "Uploads to STM32 via DFU Mode"
                }
            ],
            "tooltip": "Upload"
        },
        {
            "label": "$(notebook-delete-cell)",
            "task": "Build Clean STM",
            "tooltip": "Clean Build"
        },
        {
            "label": "$(breakpoints-view-icon)",
            "task": "Debug",
            "tooltip": "Run Debugger"
        }
    ],
    "cortex-debug.variableUseNaturalFormat": false,
    "files.associations": {
        "abstract.h": "c",
        "stm32h7xx_hal.h": "c",
        "i2c.h": "c",
        "spi.h": "c",
        "tim.h": "c",
        "ff_gen_drv.h": "c",
        "cstdio": "cpp",
        "sx1262_definitions.h": "c"
    },
    "cortex-debug.armToolchainPath": "/usr/bin",
    "cortex-debug.openocdPath": "/usr/bin/openocd",
    "stm32-for-vscode.openOCDPath": "/usr/bin/openocd"
}